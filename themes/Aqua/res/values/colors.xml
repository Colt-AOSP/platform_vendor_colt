    <!-- Fruity Pebbles overlays -->
    <color name="accent_device_default_700">#FF0DA0BA</color>
    <color name="accent_device_default_light">#FF0DA0BA</color>
    <color name="accent_device_default_dark">#FF0DA0BA</color>

    <!-- User Icons -->
    <color name="user_icon_1">#FF0DA0BA</color>
    <color name="user_icon_2">#FF8FD6E0</color>
    <color name="user_icon_3">#FF3093B5</color>
    <color name="user_icon_4">#FFA6F4c4</color>
    <color name="user_icon_5">#FF6BCE94</color>
    <color name="user_icon_6">#FFFF9572</color>
    <color name="user_icon_7">#FFB68FD8</color>
    <color name="user_icon_8">#FF0394AD</color>

    <!-- Misc overlays -->
    <color name="system_error">#FFE84C25</color>
    <color name="accent_device_default_50">#FF8EE0F2</color>
    <color name="datepicker_default_selected_text_color_material_light">#FF0DA0BA</color>
